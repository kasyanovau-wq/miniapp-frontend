{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://*.telegram.org https://telegram.org https://web.telegram.org https://tdesktop.com 'self'"
        }
      ]
    }
  ]
}
